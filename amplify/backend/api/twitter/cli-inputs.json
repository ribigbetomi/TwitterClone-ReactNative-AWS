{
  "version": 1,
  "serviceConfiguration": {
    "apiName": "twitter",
    "serviceName": "AppSync",
    "defaultAuthType": {
      "mode": "API_KEY",
      "keyDescription": "api key description",
      "expirationTime": 30
    },
    "conflictResolution": {
      "defaultResolutionStrategy": {
        "type": "AUTOMERGE"
      },
      "perModelResolutionStrategy": [
        {
          "resolutionStrategy": {
            "type": "AUTOMERGE"
          },
          "entityName": "ChatRoom"
        }
      ]
    },
    "additionalAuthTypes": [
      {
        "mode": "AWS_IAM"
      }
    ]
  }
}